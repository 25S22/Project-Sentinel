# 1. Check Core Directories
sudo ls -ld /var/ossec/
sudo ls -ld /etc/wazuh-indexer/
sudo ls -ld /usr/share/wazuh-dashboard/

# 2. Check Critical Binaries
sudo ls -l /var/ossec/bin/ossec-logtest
sudo ls -l /var/ossec/bin/wazuh-control
sudo ls -l /var/ossec/bin/agent_control

# 3. Check Service and Configuration Files
sudo ls -l /var/ossec/etc/ossec.conf
sudo ls -l /etc/wazuh-indexer/opensearch.yml
